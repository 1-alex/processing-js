var m1 = new PMatrix2D();

_checkEqual( 
[ 1.0000,  0.0000,  0.0000,
 0.0000,  1.0000,  0.0000
], m1.array() );

m1.set(1,2,3,4,5,6);
_checkEqual( 
[ 1.0000,  2.0000,  3.0000,
 4.0000,  5.0000,  6.0000
], m1.array() );

var m2 = new PMatrix2D();
m2.set(m1);
_checkEqual( 
[ 1.0000,  2.0000,  3.0000,
 4.0000,  5.0000,  6.0000
], m2.array() );

var m3 = new PMatrix2D(m1);
_checkEqual( m3.array(), m1.array() );

var m4 = new PMatrix2D(7,8,9,10,11,12);
_checkEqual( 
[ 7.0000,  8.0000,  9.0000,
 10.0000,  11.0000,  12.0000
], m4.array() );

m4.reset();

m4.skewY(0.5);
_checkEqual( 
[ 1.0000,  0.0000,  1.0000,
 0.0000,  0.5000,  0.0000
], m4.array() );

m4.reset();

m4.skewX(0.5);
_checkEqual( 
[ 1.0000,  0.0000,  1.0000,
 0.5000,  0.0000,  0.0000
], m4.array() );

m4.skewY(0.5);
_checkEqual( 
[ 1.0000,  0.0000,  2.0000,
 0.5000,  0.0000,  0.5000
], m4.array() );

m4.reset();
m4.set([1,2,3,4,5,6]);
m4.apply(16,15,14,13,12,11);
_checkEqual( 
[ 42.0000, 39.0000,  39.0000,
 129.0000,  120.0000,  117.0000
], m4.array() );

m4.reset();
m4.set([1,2,3,4,5,6]);
m4.apply([16,15,14,13,12,11]);
_checkEqual( 
[ 42.0000, 39.0000,  39.0000,
 129.0000,  120.0000,  117.0000
], m4.array() );

m4.reset();
m4.set([1,2,3,4,5,6]);
m3.set([16,15,14,13,12,11]);
m4.apply(m3);
_checkEqual( 
[ 42.0000, 39.0000,  39.0000,
 129.0000,  120.0000,  117.0000
], m4.array() );