/* 
    Processing.js Ref Test
    Function : splitTokens()
    Source   : http://processing.org/reference/splitTokens_
*/

String t = "a b";
String[] q = splitTokens(t);
String s = "a, b c ,,d "; 
String[] p = splitTokens(s, ", ");

//** REFTEST VALUES **//
background(
  reftest.checkValues([
     q[0] === "a",
     q[1] === "b",
     s[0] === "a",
     s[1] === "b",
     s[2] === "c",
     s[3] === "d",
  ])
);
exit();

